name: "Willing Accomplice"
sources: [
  "Pathfinder Roleplaying Game Ultimate Intrigue"
]
flavor_text: "Your well-placed support can aid friends and allies in maintaining their most precious secrets."
prerequisites: [
  "Cha 13"
